{
  "name": "adbb",
  "version": "1.0.0-beta",
  "description": "A little boost to the Android ADB command set to make your Android development work easier!",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node src/index.ts",
    "build": "rm -rf ./dist && tsc",
    "clean": "rm -rf ./dist",
    "deploy": "npm uninstall -g adbb && yarn build && npm install -g",
    "start": "link-module-alias node dist/index.js",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit"
  },
  "keywords": [
    "adb",
    "adbb",
    "android adb",
    "android debug bridge",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ikmich/adbb-cli"
  },
  "author": "Ikenna Agbasimalo <michfx@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@types/ip": "^1.1.0",
    "@types/node": "^14.0.13",
    "@types/yargs": "^15.0.5",
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "conf": "^6.2.4",
    "cross-env": "^7.0.2",
    "esm": "^3.2.25",
    "figlet": "^1.4.0",
    "inquirer": "^7.1.0",
    "ip": "^1.1.5",
    "link-module-alias": "^1.2.0",
    "touch": "^3.1.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsc": "^1.20150623.0",
    "tsconfig-paths": "^3.9.0",
    "tspath": "^1.3.7",
    "typescript": "^3.9.5",
    "yargs": "^15.3.1"
  },
  "bin": {
    "adbb": "./dist/index.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "babel-jest": "^26.1.0",
    "jest": "^26.1.0",
    "jest-config": "^26.1.0",
    "ts-jest": "^26.1.1"
  }
}
